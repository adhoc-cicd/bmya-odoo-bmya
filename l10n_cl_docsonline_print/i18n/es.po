#. module: l10n_cl_docsonline_print
#: model:mail.template,body_html:l10n_cl_docsonline_print.email_template_edi_invoice
msgid ""
"\n"
"            <div>\n"
"                <p>Dear ${object.partner_id.name}\n"
"                % set access_action = object."
"with_context(force_website=True).get_access_action()\n"
"                % set access_url = object.get_mail_url()\n"
"\n"
"                % if object.partner_id.parent_id:\n"
"                    (<i>${object.partner_id.parent_id.name}</i>)\n"
"                % endif\n"
"                ,</p>\n"
"                <p>Here is, in attachment, your\n"
"                % if object.display_name:\n"
"                invoice <strong>${object.display_name}</strong>\n"
"                % else:\n"
"                invoice\n"
"                % endif\n"
"                % if object.origin:\n"
"                (with reference: ${object.origin})\n"
"                % endif\n"
"                amounting in <strong>${format_amount(object.amount_total, "
"object.currency_id)}</strong>\n"
"                from ${object.company_id.name}.\n"
"                </p>\n"
"\n"
"                % if object.docs_online_token:\n"
"                    <br><br>\n"
"                    <center>\n"
"                      <a href=\"${object.docs_online_token}\" style="
"\"background-color: #1abc9c; padding: 20px; text-decoration: none; color: "
"#fff; border-radius: 5px; font-size: 16px;\" class=\"o_default_snippet_text"
"\">View Invoice</a>\n"
"                    </center>\n"
"                % endif\n"
"                    <br><br>\n"
"\n"
"                % if object.state=='paid':\n"
"                    <p>This invoice is already paid.</p>\n"
"                % else:\n"
"                    <p>Please remit payment at your earliest convenience.</"
"p>\n"
"                % endif\n"
"\n"
"                <p>Thank you,</p>\n"
"                <p style=\"color:#888888\">\n"
"                % if object.user_id and object.user_id.signature:\n"
"                    ${object.user_id.signature | safe}\n"
"                % endif\n"
"                </p>\n"
"                </div>\n"
"            "
msgstr ""
"\n"
"            <div>\n"
"                <p>Estimado(s) ${object.partner_id.name}\n"
"                % set access_action = object."
"with_context(force_website=True).get_access_action()\n"
"                % set access_url = object.get_mail_url()\n"
"\n"
"                % if object.partner_id.parent_id:\n"
"                    (<i>${object.partner_id.parent_id.name}</i>)\n"
"                % endif\n"
"                ,</p>\n"
"                <p>Aqui encontrarán en adjunto su\n"
"                % if object.display_name:\n"
"                factura <strong>${object.display_name}</strong>\n"
"                % else:\n"
"                factura\n"
"                % endif\n"
"                % if object.origin:\n"
"                (con referencia: ${object.origin})\n"
"                % endif\n"
"                por un monto de <strong>${format_amount(object.amount_total, "
"object.currency_id)}</strong>\n"
"                originada por ${object.company_id.name}.\n"
"                </p>\n"
"\n"
"                % if object.docs_online_token:\n"
"                    <br><br>\n"
"                    <center>\n"
"                      <a href=“${object.docs_online_token}” "
"style=“background-color: #1abc9c; padding: 20px; text-decoration: none; "
"color: #fff; border-radius: 5px; font-size: 16px;” "
"class=“o_default_snippet_text”>Ver Factura en www.documentosonline.cl</a>\n"
"                    </center>\n"
"                % endif\n"
"                    <br><br>\n"
"\n"
"                % if object.state=='paid':\n"
"                    <p>Esta factura ya se encuentra pagada.</p>\n"
"                % else:\n"
"                    <p>Quedamos a la espera de su pago.</p>\n"
"                % endif\n"
"\n"
"                <p>Gracias,</p>\n"
"                <p style=“color:#888888”>\n"
"                % if object.user_id and object.user_id.signature:\n"
"                    ${object.user_id.signature | safe}\n"
"                % endif\n"
"                </p>\n"
"                </div>\n"
"            "

